run = "uvicorn main:app --host 0.0.0.0 --port 8000"
entrypoint = "main.py"
modules = ["python-3.11"]

[nix]
channel = "stable-24_05"
packages = ["gitFull", "playwright-driver"]

[deployment]
run = ["sh", "-c", "uvicorn main:app --host 0.0.0.0 --port 8000"]
deploymentTarget = "cloudrun"

[[ports]]
localPort = 8000
externalPort = 80

[agent]
expertMode = true
